        <PackageId>FastReport.CoreWin.Data.Postgres</PackageId>
        <PackageIconUrl>https://www.fast-report.com/download/images/frlogo-big.png</PackageIconUrl>
        <PackageTags>reporting, postgres, connection, reports</PackageTags>
